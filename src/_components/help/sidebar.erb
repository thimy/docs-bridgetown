<bump-sidebar>
  <%= dsd do %>
    <aside>
      <ul class="sidebar-list">
        <% @sidebar.help.each do |page| %>
          <%= render Help::Sidebar::Item.new(page: page, page_list: @page_list, current: @current) %>
        <% end %>
      </ul>
    </aside>
    <%= dsd_style %>
  <% end %>
</bump-sidebar>
